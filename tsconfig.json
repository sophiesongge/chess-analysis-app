{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ]
    },
    "strict": true,
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noEmit": true,
    "skipLibCheck": true,
    "target": "esnext",
    "lib": [
      "esnext"
    ]
  },
  "extends": "expo/tsconfig.base"
}
